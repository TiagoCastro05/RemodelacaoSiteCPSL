{
  "name": "cpsl-website",
  "version": "1.0.0",
  "description": "Website institucional do Centro Paroquial e Social de Lanheses",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "predev": "npx kill-port 3000 4000 || true",
    "dev": "concurrently \"nodemon server/server.js\" \"cd client && npm start\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "cpsl",
    "react",
    "postgresql",
    "supabase",
    "cms"
  ],
  "author": "CPSL",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.43.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.2",
    "supabase": "^2.76.12"
  }
